        {
          "id": "/page/0/SectionHeader/5",
          "block_type": "SectionHeader",
          "html": "<h2><span id=\"page-0-5\"></span>1 Introduction</h2>",
          "polygon": [
            [
              108.0,
              376.27734375
            ],
            [
              190.8136749267578,
              376.27734375
            ],
            [
              190.8136749267578,
              388.8955383300781
            ],
            [
              108.0,
              388.8955383300781
            ]
          ],
          "bbox": [
            108.0,
            376.27734375,
            190.8136749267578,
            388.8955383300781
          ],
          "children": null,
          "section_hierarchy": {
