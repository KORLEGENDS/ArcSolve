        {
          "id": "/page/0/SectionHeader/3",
          "block_type": "SectionHeader",
          "html": "<h2>Abstract</h2>",
          "polygon": [
            [
              282.09375,
              201.09375
            ],
            [
              328.2432861328125,
              201.09375
            ],
            [
              328.2432861328125,
              213.12255859375
            ],
            [
              282.09375,
              213.12255859375
            ]
          ],
          "bbox": [
            282.09375,
            201.09375,
            328.2432861328125,
